{
  "name": "observable-webworker",
  "version": "0.0.0-development",
  "license": "MIT",
  "scripts": {
    "------------------ CORE COMMANDS -----------------": "",
    "ng": "ng",
    "------------------ LINT COMMANDS -----------------": "",
    "prettier": "prettier",
    "prettier:base": "yarn run prettier \"**/*.{js,json,scss,ts,html}\"",
    "prettier:write": "yarn run prettier:base --write",
    "prettier:check": "yarn run prettier:base --list-different",
    "------------------ DEMO COMMANDS -----------------": "",
    "demo:start": "yarn run ng serve",
    "demo:build:base": "yarn run ng build",
    "demo:build:prod": "yarn run demo:build:base --prod",
    "demo:test": "yarn run ng test",
    "demo:test:e2e:watch": "yarn run cy open",
    "demo:test:e2e:ci": "yarn run cy run",
    "demo:lint:check": "yarn run ng lint",
    "demo:lint:fix": "yarn run demo:lint:check --fix",
    "------------------ LIB COMMANDS -----------------": "",
    "lib:build:prod": "yarn run ng build --project observable-webworker --prod",
    "lib:build:watch": "yarn run lib:build:prod --watch",
    "lib:test:watch": "yarn run ng test --project observable-webworker",
    "lib:test:ci": "yarn run ng test --project observable-webworker --watch false --code-coverage",
    "------------------ RELEASE COMMANDS ------------------": "",
    "semantic-release": "semantic-release",
    "readme:build": "embedme README.md",
    "readme:check": "yarn readme:build && ! git status | grep README.md || (echo 'You must commit build and commit changes to README.md!' && exit 1)",
    "report-coverage": "codecov",
    "------------------ QUICK COMMANDS ------------------": "",
    "lint:fix": "yarn demo:lint:fix && yarn prettier:write",
    "test": "yarn lib:test:watch --code-coverage",
    "start": "yarn demo:start",
    "commit": "git add . && git-cz",
    "betterer": "betterer"
  },
  "dependencies": {
    "@angular/animations": "~11.2.14",
    "@angular/common": "~11.2.14",
    "@angular/compiler": "~11.2.14",
    "@angular/core": "~11.2.14",
    "@angular/forms": "~11.2.14",
    "@angular/platform-browser": "~11.2.14",
    "@angular/platform-browser-dynamic": "~11.2.14",
    "@angular/router": "~11.2.14",
    "google-charts": "2.0.0",
    "js-md5": "0.7.3",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.18",
    "@angular/cli": "~11.2.18",
    "@angular/compiler-cli": "~11.2.14",
    "@angular/language-service": "~11.2.14",
    "@betterer/cli": "^1.1.3",
    "@betterer/typescript": "^1.1.2",
    "@types/google.visualization": "0.0.48",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/js-md5": "^0.4.2",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "commitizen": "4.0.3",
    "cypress": "3.4.1",
    "cz-conventional-changelog": "2.1.0",
    "embedme": "1.6.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.11",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.2.4",
    "prettier": "2.2.1",
    "semantic-release": "^17.2.3",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cloudnc/observable-webworker.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}